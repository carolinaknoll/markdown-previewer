{
  "name": "markdown-previewer",
  "version": "1.0.0",
  "description": "Online Markdown Previewer, part of the FreeCodeCamp Frontend Libraries projects.",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --config webpack.dev.js --mode development --open --hot",
